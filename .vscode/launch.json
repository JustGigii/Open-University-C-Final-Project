{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": ["onlymcro2.sh"],
      "stopAtEntry": false,
      "externalConsole": true,
      "cwd": "c:/Open-University-C-Final-Project",
      "program": "c:/Open-University-C-Final-Project/program.exe",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}